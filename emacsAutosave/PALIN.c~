#include<stdio.h>
#include<stdlib.h>
#include<assert.h>
#include<string.h>
#include<math.h>

#define ASSERTS

#define ASSERT(x) assert(x)

void reverse(char *str){
#ifdef ASSERTS
  assert(str != NULL);
  int strLen = strlen(str);
#endif

  int start = 0;
  int end = strlen(str) - 1;

#ifdef ASSERTS
  assert(start == 0);
  assert(end == strlen(str) - 1);
#endif

  while (start <=  end){
#ifdef ASSERTS
    assert(start <= end);
#endif
    char temp = str[start];
    str[start] = str[end];
    str[end] = temp;
    start++;
    end--;
  }
#ifdef ASSERTS
  assert(strLen == strlen(str));
  assert(end <= start);
#endif

}

char* itoa(int n){
  char *retVal;
  int counter = 0;

  if (n == 0){
    retVal = (char *)malloc(sizeof(char) * 2);

    retVal[counter++] = '0';
  }else{

  }
  
  retVal[counter] = '\0';
  return retVal;
}



