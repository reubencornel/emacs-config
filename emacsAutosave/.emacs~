;; All the versions of emacs on my computer
(load (expand-file-name "~/emacs/my-emacs-config/utility-code.el"))
(load (expand-file-name "~/emacs/my-emacs-config/mode-specific-config.el"))

(when (aquamacs-p)
  (one-buffer-one-frame-mode 0)
  (setq mac-command-modifier 'meta))

(when (gnu-emacs-p)
  (setq mac-command-modifier 'meta))

(when (or (carbon-emacs-p)
	  (aquamacs-p))
  (load (expand-file-name "~/emacs/my-emacs-config/other-emacs-init.el")))

(when (and (gnu-emacs-p)
	   (not (carbon-emacs-p)))
  (load (expand-file-name "~/emacs/my-emacs-config/gnu-emacs-init.el")))

(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(icicle-default-value (quote preselect-end))
 '(icicle-use-anything-candidates-flag t)
 '(ido-mode (quote both) nil (ido))
 '(org-agenda-files (quote ("~/Courses/Research/Thesis/document.org" "~/organizer.org")))
 '(synonyms-cache-file "/Users/reuben/emacs/mthesaur.txt.cache")
 '(synonyms-file "/Users/reuben/emacs/mthesaur.txt"))
